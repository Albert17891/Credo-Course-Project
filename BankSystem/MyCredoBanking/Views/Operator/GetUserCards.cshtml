@model IEnumerable<CreditCardResponse>

@{
    ViewData["Title"] = "CreditCard Page";
}

<table class="table">
    <thead>
        <tr>
            <th>
               ბარათის ნომერი
            </th>
            <th>
                ბარათის ვადა
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cvv)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pin)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CardNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CardExpireDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cvv)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pin)
                </td>              
            </tr>
        }        
    </tbody>
   
</table>
